{% extends 'layouts/base.twig' %}

 {% block main_content %}
 <h2>All Images</h2>
 <ul>
    {% for image in images %}
        <li>
          {{image.id}}
            {{variant.title}} - 
                <a href="/images/{{image.id}}">Edit</a>
                <a href="/images/{{image.id}}/delete">Delete</a>
        </li>
    {% endfor %}
 </ul>
   <a href="/images/new">Add New Image</a>
 {% endblock %}